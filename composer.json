{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~4.0",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "jlapp/swaggervel": "dev-master",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "rap2hpoutre/laravel-log-viewer": "^0.21.0",
        "thomaswelton/laravel-gravatar": "^1.1",
        "yajra/laravel-datatables-buttons": "^4.0",
        "yajra/laravel-datatables-editor": "^1.4",
        "yajra/laravel-datatables-fractal": "^1.2",
        "yajra/laravel-datatables-html": "^3.7",
        "yajra/laravel-datatables-oracle": "^8.4.1"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0",
        "laravel/dusk": "^3.0.1",
        "sami/sami": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php",
            "app/Helpers/channelconf.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php /home/vagrant/repos/manager/vendor/sami/sami/sami.php update sami.config.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
